{
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "run-s validate build:rollup build:custom-properties build:html build:panel-options",
    "build:rollup": "rollup -c rollup.config.js --environment NODE_ENV:production",
    "build:custom-properties": "node --experimental-json-modules ./scripts/customProperties.mjs",
    "build:html": "node ./scripts/html.mjs",
    "build:panel-options": "node ./scripts/panelOptions.mjs",
    "dev": "vite",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "eslint --fix .",
    "fix:prettier": "prettier --write .",
    "lint": "run-s lint:eslint lint:prettier",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "validate": "run-s typecheck lint",
    "watch": "node ./scripts/watch.js"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@grafana/data": "^10.1.1",
    "@grafana/runtime": "^10.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "autoprefixer": "^10.4.16",
    "babel-preset-solid": "^1.8.4",
    "chokidar": "^3.5.3",
    "cssnano": "^6.0.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-solid": "^0.13.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "rollup": "^4.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "solid-js": "^1.8.5",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-solid": "2.7.2",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
