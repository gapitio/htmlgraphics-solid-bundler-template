{
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "run-s validate build:rollup build:custom-properties build:html build:panel-options",
    "build:rollup": "rollup -c rollup.config.js --environment NODE_ENV:production",
    "build:custom-properties": "node --experimental-json-modules ./scripts/customProperties.mjs",
    "build:html": "node ./scripts/html.mjs",
    "build:panel-options": "node ./scripts/panelOptions.mjs",
    "dev": "vite",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "eslint --fix .",
    "fix:prettier": "prettier --write .",
    "lint": "run-s lint:eslint lint:prettier",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "validate": "run-s typecheck lint"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@grafana/data": "^9.1.6",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "autoprefixer": "^10.4.12",
    "babel-preset-solid": "^1.5.6",
    "cssnano": "^5.1.13",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-solid": "^0.7.3",
    "eslint-plugin-unicorn": "^43.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "rollup": "^2.79.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.0",
    "solid-js": "^1.5.6",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.2",
    "vite": "^3.1.3",
    "vite-plugin-solid": "2.1.2",
    "vite-tsconfig-paths": "^3.5.0"
  }
}
